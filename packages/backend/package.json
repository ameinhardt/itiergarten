    "@types/node": "^18.6.4",
    "@types/supertest": "^2.0.12",
    "@types/ws": "^8.5.3",
    "eslint": "^8.21.0",
    "jest": "^28.1.3",
    "jest-environment-node": "^28.1.3",
    "jest-json-schema": "^6.1.0",
    "lint-staged": "^13.0.3",
    "mongodb": "^4.7.0",
    "nock": "^13.2.9",
    "nodemon": "^2.0.19",
    "rollup": "^2.77.0",
    "rollup-plugin-terser": "^7.0.2",
    "source-map-support": "^0.5.21",
