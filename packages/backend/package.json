    "@koa/router": "^12.0.0",
    "ajv": "^8.11.0",
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "grant": "^5.4.21",
    "https-proxy-agent": "5.0.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.1.3",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-compose": "^4.1.0",
    "koa-compress": "^5.1.0",
    "koa-conditional-get": "^3.0.0",
    "koa-etag": "^4.0.0",
    "koa-helmet": "^6.1.0",
